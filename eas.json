{
  "cli": {
    "version": ">= 16.3.3",
    "appVersionSource": "local"
  },
  "build": {
    "debug": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "debug",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "debug"
      }
    },
    "stage": {
      "distribution": "internal",
      "channel": "stage",
      "env": {
        "APP_VARIANT": "stage",
      }
    },
    "production": {
      "autoIncrement": false,
      "channel": "production",
      "env": {
        "APP_VARIANT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
